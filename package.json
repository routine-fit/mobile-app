{
  "name": "routinefit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn android:start.dev",
    "android:start.dev": "react-native run-android --mode=devDebug",
    "android:start.prod": "react-native run-android --mode=prodDebug",
    "android:build.prod": "cd android/ && ./gradlew clean bundleProductionRelease",
    "ios": "yarn ios:start.dev",
    "ios:start.dev": "react-native run-ios --scheme RoutineFit-Dev --simulator=\"iPhone SE (3rd generation)\"",
    "ios:start.prod": "react-native run-ios --scheme routinefit-prod --simulator=\"iPhone SE (3rd generation)\"",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky install",
    "pod:install": "cd ios && pod install && cd ..",
    "clear:modules": "rm -rf node_modules && yarn",
    "set:core": "yarn clear:modules && yarn pod:install && yarn start"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.15",
    "@gluestack-ui/config": "^1.0.3",
    "@gluestack-ui/themed": "^1.0.13",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.1",
    "i18next": "^23.7.16",
    "lucide-react-native": "^0.308.0",
    "react": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^14.0.0",
    "react-native": "0.72.6",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "2.12.1",
    "react-native-localize": "^3.0.5",
    "react-native-reanimated": "3.4.2",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.1.0",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.4",
    "reactotron-redux": "^3.1.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
